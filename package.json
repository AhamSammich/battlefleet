{
  "name": "ts-battlefleet",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "test": "jest",
    "start": "node app.js",
    "dev": "vite",
    "start-server": "cd server && npm run start",
    "dev:start-server": "npm-run-all -p dev start-server",
    "prod:start-server": "npm-run-all -p start start-server",
    "build": "vue-tsc --noEmit && vite build",
    "move-icon": "mv dist/favicon.ico dist/assets/",
    "build-move": "npm-run-all build move-icon",
    "preview": "npm-run-all -p start-server start"
  },
  "dependencies": {
    "express": "^4.18.1",
    "npm-run-all": "^4.1.5",
    "socket.io-client": "^4.5.1",
    "vue": "^3.2.25"
  },
  "devDependencies": {
    "@types/jest": "^28.1.6",
    "@vitejs/plugin-vue": "^2.3.3",
    "jest": "^28.1.3",
    "ts-jest": "^28.0.7",
    "typescript": "^4.5.4",
    "vite": "^2.9.9",
    "vue-tsc": "^0.34.17"
  }
}
